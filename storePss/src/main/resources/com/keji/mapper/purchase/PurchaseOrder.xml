<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.keji.mapper.purchase.PurchaseOrderMapper">
    <resultMap id="purchaseOrder" type="PurchaseOrder">
        <id column="tpoId" property="id"/>
        <result column="tpoCreateDate" property="createDate"/>
        <result column="tpoProvider" property="provider"/>
        <result column="tpoOperationEmp" property="operationEmp"/>
        <result column="tpoPrice" property="price"/>
        <result column="tpoCreateEmp" property="createEmp"/>
        <result column="tpoRemark" property="remark"/>
        <result column="stoSate" property="sate"/>
        <collection property="PurchaseOrderDetailList" ofType="PurchaseOrderDetail">
            <id column="" property="id"/>
            <result column="" property="posId"/>
            <result column="" property="gooId"/>
            <result column="" property="tpod_gooNum"/>
            <result column="" property="tpod_gooTax"/>
            <result column="" property="tpod_gooTaxPrc"/>
            <result column="" property="tpod_gooOtherPrice"/>
            <result column="" property="tpod_gooTotalPrice"/>
            <result column="" property="tpod_gooRemark"/>
        </collection>
    </resultMap>
    <select id="queryPurchaseOrder"></select>


</mapper>